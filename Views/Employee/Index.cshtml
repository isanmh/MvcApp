<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Nama Lengkap</th>
            <th scope="col">Divisi</th>
            <th scope="col">Role</th>
            <th scope="col">Aksi</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var employee in Model)
        {
            <tr>
                <th scope="row">1</th>
                <td>@employee.Name</td>
                <td>@employee.Division</td>
                <td>@employee.Role</td>
                <td>btn aksi</td>
            </tr>
        }
    </tbody>
</table>